public class Ex6 {
  public static void main(String args[]) {
  System.out.println("Enter numbers:");
  Scanner sc = new Scanner(System.in);
  int a = sc.nextInt();
  int b = sc.nextInt();
  int sum = a+b;
  int diff = a-b;
  int mul = a*b;
  int div = a/b;
  int rem = a%b;
  System.out.println("Sum=" + sum, "Difference = " + diff, "Multiply = " + mul, "Division =" + div, "Reminder = " + rem);
  }
}
