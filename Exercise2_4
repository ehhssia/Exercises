public class Ex4_2 {
	static boolean checknumber(int n) {
		if (n==0)
			return false;
		
		while(n!=1) {
			if (n%2!=0)
				return false;
			n=n/2;
		}
		return true;
	}
	public static void main (String args[]) {
		if (checknumber(64))
			System.out.println("yes");
		else
			System.out.println("no");
	}
